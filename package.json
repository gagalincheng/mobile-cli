{
  "name": "mobile-cli",
  "scripts": {
    "start": "npm-run-all --parallel client debug",
    "debug": "egg-bin debug --port=13810 --inspect=9200",
    "client": "cross-env NODE_ENV=development node --max-old-space-size=8012 ./internals/scripts/start-client.js",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf ./build",
    "build": "cross-env NODE_ENV=production ASSET_ENV=production node --max-old-space-size=8012 ./internals/scripts/build.js",
    "build:test": "cross-env NODE_ENV=production ASSET_ENV=test node --max-old-space-size=8012 ./internals/scripts/build.js",
    "server": "eggctl start --workers=1 --port=13800 --title=school-web-mobile",
    "stop": "eggctl stop --title=school-web-mobile",
    "g": "plop --plopfile internals/generators/index.js"
  },
  "hostPrefix": "/",
  "assetUrl": {
    "development": "/statics/",
    "beta": "",
    "test": "",
    "production": ""
  },
